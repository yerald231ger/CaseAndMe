@model CategoryFilter
@for (int i = 0; i < Model.CategoriesFilters.Count; i++)
{
    <ul>
        <li>
            <input asp-for="@Model.CategoriesFilters[i].Selected" type="checkbox" onclick="submit()" style="display:initial"/>
            @Html.ValueFor(m => m.CategoriesFilters[i].Value)
            (@Html.ValueFor(m => m.CategoriesFilters[i].Matched))

            @*Inputs Escondidos*@
            <input asp-for="@Model.CategoriesFilters[i].Value" type="hidden" />
            <input asp-for="@Model.CategoriesFilters[i].Matched" type="hidden" />
            <input asp-for="@Model.CategoriesFilters[i].Selected" type="hidden" />
        </li>
    </ul>
}